{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "methods": ["OPTIONS"],
      "continue": true,
      "headers": {
        "Access-Control-Allow-Origin": "https://vanni-test-frontend.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Session-ID, X-Request-ID, X-Cancel-Previous",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Max-Age": "86400"
      }
    },
    {
      "src": "/api/chat",
      "dest": "/_middleware"
    },
    {
      "src": "/api/react-search",
      "dest": "/_middleware"
    },
    {
      "src": "/api/react-search-streaming",
      "dest": "/_middleware"
    },
    {
      "src": "/api/related-questions",
      "dest": "/_middleware"
    },
    {
      "src": "/api/health",
      "dest": "/_middleware"
    },
    {
      "src": "/api/models",
      "dest": "/_middleware"
    },
    {
      "src": "/api/upload",
      "dest": "/_middleware"
    }
  ]
}
